{
  "name": "Pure Password Manager",
  "version": "0.0.8",
  "manifest_version": 2,
  "description": "generate website specific password from your main password",
  "homepage_url": "https://github.com/Bramas/pure-password-manager",
  "icons": {
    "144": "assets/icon-144.png"
  },
  "default_locale": "en",
  "background": {
    "scripts": ["background.bundle.js"],
    "persistent": false
  },
  "options_page": "options_custom/index.html",
  "page_action": {
    "default_icon": "assets/icon-144.png",
    "default_title": "Pure PM",
    "default_popup": "popup.html"
  },
  "permissions": [
    "declarativeContent",
    "storage",
    "activeTab"
  ]
}
